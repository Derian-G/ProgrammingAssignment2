MAKECACHEMATRIX
> makeCacheMatrix <- function(x = matrix()) {
+     inv_x<- NULL
+     set <- function(j) {
+         x <<- j
+         inv_x <<- NULL
+     }
+     get <- function() x
+     setinv <- function(inverse) inv_x <<- inverse
+     getinv <- function() inv_x
+     list(set = set,
+          get = get,
+          setinv = setinv,
+          getinv = getinv)
+ }
